-- LocalScript di dalam Frame

local frame = script.Parent
local keyInput1 = Instance.new("TextBox")
local keyInput2 = Instance.new("TextBox")
local enterButton = Instance.new("TextButton")
local correctKey1 = "KUNCI1" -- Ganti dengan kunci yang diinginkan
local correctKey2 = "KUNCI2" -- Ganti dengan kunci yang diinginkan

-- Mengatur TextBox untuk input kunci pertama
keyInput1.Size = UDim2.new(0.8, 0, 0.1, 0)
keyInput1.Position = UDim2.new(0.1, 0, 0.1, 0)
keyInput1.PlaceholderText = "Masukkan Kunci 1"
keyInput1.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
keyInput1.Parent = frame

-- Mengatur TextBox untuk input kunci kedua
keyInput2.Size = UDim2.new(0.8, 0, 0.1, 0)
keyInput2.Position = UDim2.new(0.1, 0, 0.25, 0)
keyInput2.PlaceholderText = "Masukkan Kunci 2"
keyInput2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
keyInput2.Parent = frame

-- Mengatur tombol enter
enterButton.Size = UDim2.new(0.8, 0, 0.1, 0)
enterButton.Position = UDim2.new(0.1, 0, 0.4, 0)
enterButton.Text = "Masuk"
enterButton.BackgroundColor3 = Color3.fromRGB(0, 102, 204)
enterButton.TextColor3 = Color3.fromRGB(255, 255, 255)
enterButton.Parent = frame

-- Fungsi untuk menangani klik tombol
local function onButtonClick(buttonName)
    print(buttonName .. " diklik!")
    -- Tambahkan logika yang diinginkan di sini
end

-- Fungsi untuk memeriksa kunci
local function checkKeys()
    if keyInput1.Text == correctKey1 and keyInput2.Text == correctKey2 then
        frame.Visible = true
        keyInput1.Visible = false
        keyInput2.Visible = false
        enterButton.Visible = false
        print("Kunci benar! UI dibuka.")
        
        -- Membuat tombol utama
        for i = 1, 6 do
            local button = Instance.new("TextButton")
            button.Size = UDim2.new(0.8, 0, 0.1, 0) -- Ukuran tombol
            button.Position = UDim2.new(0.1, 0, (i - 1) * 0.15 + 0.5, 0) -- Posisi tombol
            button.Text = "Tombol " .. i
            button.BackgroundColor3 = Color3.fromRGB(0, 102, 204) -- Warna tombol
            button.TextColor3 = Color3.fromRGB(255, 255, 255) -- Warna teks
            button.Parent = frame

            -- Event ketika tombol diklik
            button.MouseButton1Click:Connect(function()
                onButtonClick(button.Text)
            end)
        end
    else
        print("Kunci salah! Silakan coba lagi.")
        keyInput1.Text = "" -- Kosongkan TextBox jika kunci salah
        keyInput2.Text = "" -- Kosongkan TextBox jika kunci salah
    end
end

-- Event ketika tombol enter diklik
enterButton.MouseButton1Click:Connect(checkKeys)

-- Meng
